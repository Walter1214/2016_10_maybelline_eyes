        script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js")
        //- script(defer,src="./asset/js/main.js")
        script(defer,src="./asset/js/vendor/all.js")
        script(defer,src="./asset/js/vendor/loadCSS.js")
        
        script(defer src="//cdnjs.cloudflare.com/ajax/libs/device.js/0.2.7/device.min.js")

        //- link(rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.css")
        script(defer src="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js")
        
        block head
    body
        .wrap
            header
                a.logo(href="https://www.maybelline.com.tw/",onclick="$.gapv('/style2016/guide/m_home_btn');")
