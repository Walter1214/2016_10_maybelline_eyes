        title=title
        meta(property='og:title' content='#{title}')
        meta(property='og:description' content='#{descript}')
        meta(property='og:image' content='asset/img/FB_share.jpg')
        meta(property='og:type' content='website')
        meta(name='description' content='#{descript}')
        meta(name='keywords' content='媚比琳,Maybelline,媚比琳超激細抗暈眼線液,極黑,極細,0.01mm流暢筆尖,防水,抗汗,最佳主角由你主眼')
